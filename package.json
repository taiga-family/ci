{
    "name": "taiga-ui-ci",
    "version": "1.87.2",
    "private": true,
    "license": "Apache-2.0",
    "scripts": {
        "prettier": "prettier '**/*.{svg,yml,js,ts,html,md,less,json}'",
        "format": "npm run prettier -- --write",
        "release": "npx syncer"
    },
    "prettier": "@taiga-ui/prettier-config",
    "devDependencies": {
        "@taiga-ui/auto-changelog-config": "0.219.0",
        "@taiga-ui/prettier-config": "0.219.0",
        "@taiga-ui/release-it-config": "0.219.0",
        "@taiga-ui/syncer": "0.219.0"
    },
    "syncer": {
        "includePaths": [
            "./package-lock.json"
        ],
        "matchPackageNames": [
            "taiga-ui-ci"
        ],
        "ignorePackageNames": []
    }
}
